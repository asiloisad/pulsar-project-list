'atom-workspace':
  'f10'     : 'project-list:toggle'
  'ctrl-f10': 'project-list:edit'
  'alt-f10' : 'project-list:recent'

'.project-list atom-text-editor[mini]':
  'enter'      : 'project-list:open'
  'alt-enter'  : 'project-list:swap'
  'shift-enter': 'project-list:append'
  'alt-v'      : 'project-list:paste'
  'f5'         : 'project-list:update'
